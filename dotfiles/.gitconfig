[color]
	diff = auto
	status = auto
	branch = auto
[alias]
  ll = log --graph --decorate --abbrev-commit --stat -M
  st = status
  diffw = diff --color-words
  ready = rebase -i @{u}
  root = !pwd
#[user]
#  name = ...
#  email = ...
[include]
  path = .gitconfiglocal
[diff]
	renames = true
[push]
	default = simple
